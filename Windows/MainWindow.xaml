<Window x:Class="Windows.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Windows"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">

    <StackPanel>
        <Button Click="Button_Click">Click this button</Button>
        <Button Click="Open_Activate_Window">Open and activate a window</Button>
        <Button Click="Open_notActivate_Window">Open but not activate a window</Button>
    </StackPanel>
    
</Window>
